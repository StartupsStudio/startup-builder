{
  "name": "startup-builder",
  "version": "0.1.0",
  "description": "Build autonomous startups with Business-as-Code",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./api": {
      "import": "./dist/api.js",
      "types": "./dist/api.d.ts"
    },
    "./db": {
      "import": "./dist/db.js",
      "types": "./dist/db.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/api.ts src/db.ts --format esm --dts",
    "dev": "tsup src/index.ts src/api.ts src/db.ts --format esm --dts --watch",
    "test": "vitest",
    "lint": "eslint src/",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "rpc.do": "latest"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "keywords": [
    "startup",
    "builder",
    "business-as-code",
    "autonomous",
    "ai",
    "agents",
    "workflow"
  ],
  "author": "Startups.Studio",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/StartupsStudio/startup-builder.git"
  },
  "homepage": "https://startup.builder",
  "bugs": {
    "url": "https://github.com/StartupsStudio/startup-builder/issues"
  }
}
